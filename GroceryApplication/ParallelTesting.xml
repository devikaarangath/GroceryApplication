<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Paralleltesting" parallel="methods">
<parameter name="browsers" value="Chrome"></parameter>
  <test thread-count="5" name="Test1">
  
    <classes>
      <class  name="testscript.HomeTest">
      
      <methods>
      <include name="verifyUserIsAbleToLogOut"></include>
      </methods>
      </class>
      
    </classes>
  </test> <!-- Test -->
   <test thread-count="5" name="Test2">
  
    <classes>
      <class  name="testscript.LoginTest">
      <methods>
      <include name="verifyUserWithValidCredenetials"></include>
      </methods>
      </class>
      
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
